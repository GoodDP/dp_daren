<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>SSH_Demo</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>TopUsers</servlet-name>
		<servlet-class>com.dianping.servlet.TopUsers</servlet-class>
	</servlet>
	
	<!-- 搜索结果页面 -->
	<servlet>
		<servlet-name>ResearchResult</servlet-name>
		<servlet-class>com.dianping.servlet.ResearchResult</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>UserInfo</servlet-name>
		<servlet-class>com.dianping.servlet.UserInfo</servlet-class>
	</servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetUserByShopId</servlet-name>
    <servlet-class>com.dianping.servlet.GetUserByShopId</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetFootListById</servlet-name>
    <servlet-class>com.dianping.servlet.GetFootListById</servlet-class>
  </servlet>


	
	
	<servlet-mapping>
		<servlet-name>UserInfo</servlet-name>
		<url-pattern>/userInfo</url-pattern>
	</servlet-mapping>
	
	
	
	
	
	
	
	<servlet-mapping>
		<servlet-name>TopUsers</servlet-name>
		<url-pattern>/topUsers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResearchResult</servlet-name>
		<url-pattern>/researchResult</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetUserByShopId</servlet-name>
    <url-pattern>/GetUserByShopId</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetFootListById</servlet-name>
    <url-pattern>/GetFootListById</url-pattern>
  </servlet-mapping>
  
  
</web-app>